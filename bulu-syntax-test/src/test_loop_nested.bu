// Test des boucles imbriquées

func main() {
    println("=== Test des boucles imbriquées ===")
    
    // Boucles for imbriquées
    println("Boucles for imbriquées:")
    for i in 1..4 {
        for j in 1..4 {
            println("i=" + i.toString() + ", j=" + j.toString())
        }
    }
    
    // Boucle while dans for
    println("\nBoucle while dans for:")
    for i in 1..3 {
        let j = 0
        while j < i {
            println("Outer i=" + i.toString() + ", Inner j=" + j.toString())
            j = j + 1
        }
    }
    
    println("Test terminé")
}