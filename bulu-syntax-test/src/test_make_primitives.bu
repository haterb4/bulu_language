// Test make() avec différents types primitifs et non primitifs
struct Person {
    name: string
    priv age: uint32

    func great(name: string): string {
        return this.name
    }

    func sayAge(): uint32 {
        return this.age
    }
}

interface Speaker {
    func great(name: string): string
    func sayAgage(): uint32
}

func main() {
    println("=== Test make() avec types primitifs ===")
    
    // Test avec int32
    println("Test 1: make(int32)")
    let x = make(int32)
    println("Résultat int32: " + x)
    
    // Test avec string
    println("Test 2: make(string)")
    let s = make(string)
    println("Résultat string: " + s)
    
    // Test avec bool
    println("Test 3: make(bool)")
    let b = make(bool)
    println("Résultat bool: " + b)
    
    // Test avec slice
    println("Test 4: make([]int32)")
    let arr = make([]int32)
    println("Résultat slice: " + arr)
    
    // Test avec slice et taille
    println("Test 5: make([]int32, 5)")
    let arr2 = make([]int32, 5)
    println("Résultat slice avec taille: " + arr2)

    // Test avec slice de struct
    println("Test 6: make([]Person)")
    let pers1 = make([]Person)
    println("Résultat slice: " + pers1)

    // Test avec slice et taille sur struct
    println("Test 7: make([]int32, 5)")
    let pers2 = make([]Person, 5)
    println("Résultat slice avec taille: " + pers2)

    // Test avec slice de interface
    println("Test 6: make([]Speaker)")
    let great1 = make([]Person)
    println("Résultat slice: " + pers1)

    // Test avec slice et taille sur interface
    println("Test 9: make([]int32, 5)")
    let great2 = make([]Speaker, 5)
    println("Résultat slice avec taille: " + pers2.toString())
    
    println("Tests terminés")
}