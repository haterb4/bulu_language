// Test de méthode privée

struct TestStruct {
    value: float64
    
    priv func checkValue(x: float64): bool {
        println("checkValue (privée) appelé avec x = " + x.toString())
        return x > 0.0
    }
    
    pub func testMethod(x: float64): string {
        println("testMethod (publique) appelé avec x = " + x.toString())
        if this.checkValue(x) {
            return "positif"
        } else {
            return "négatif"
        }
    }
}

func main() {
    println("=== Test Private Method ===")
    
    let s = TestStruct{value: 42.0}
    println("Structure créée")
    
    let result = s.testMethod(3.0)
    println("Résultat: " + result)
    
    println("Test terminé")
}