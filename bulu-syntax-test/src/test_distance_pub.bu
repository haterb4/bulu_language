// Test distance avec pub

struct Point {
    x: float64
    y: float64
    
    pub func distance(): float64 {
        return this.x ** 2 + this.y ** 2
    }
}

func main() {
    println("=== Test Distance Pub ===")
    
    let p = Point{x: 3.0, y: 4.0}
    println("Point créé")
    
    let dist = p.distance()
    println("Distance: " + dist.toString())
    
    println("Test terminé")
}