// Test d'appel de méthode avec arguments

struct TestStruct {
    value: float64
    
    func checkValue(x: float64): bool {
        println("checkValue appelé avec x = " + x.toString())
        return x > 0.0
    }
    
    func testMethod(x: float64): string {
        println("testMethod appelé avec x = " + x.toString())
        if this.checkValue(x) {
            return "positif"
        } else {
            return "négatif"
        }
    }
}

func main() {
    println("=== Test Method with Args ===")
    
    let s = TestStruct{value: 42.0}
    println("Structure créée")
    
    let result = s.testMethod(3.0)
    println("Résultat: " + result)
    
    println("Test terminé")
}