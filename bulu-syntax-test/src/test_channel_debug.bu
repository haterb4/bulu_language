func main() {
    println("=== Test debug channel ===")
    
    let ch = make(chan int32, 1)
    println("Channel créé: " + ch.toString())
    
    // Test simple d'envoi
    println("Avant envoi")
    ch <- 42
    println("Après envoi")
    
    // Test simple de réception
    println("Avant réception")
    let val = <-ch
    println("Après réception, valeur: " + val.toString())
}