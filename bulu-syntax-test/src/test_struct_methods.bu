// Test des méthodes de structures

struct Calculator {
    value: float64
    
    func add(x: float64): float64 {
        return this.value + x
    }
    
    func getValue(): float64 {
        return this.value
    }
}

func main() {
    println("=== Test des Méthodes de Structures ===")
    
    let calc = Calculator{value: 10.0}
    println("Calculator créé avec value = " + calc.value.toString())
    
    // Test d'accès à une méthode
    let method = calc.getValue
    println("Méthode récupérée")
    
    // Test d'appel de méthode
    let result = calc.getValue()
    println("Résultat de getValue(): " + result.toString())
    
    // Test d'appel de méthode avec paramètre
    let addResult = calc.add(5.0)
    println("Résultat de add(5.0): " + addResult.toString())
    
    println("Test terminé")
}