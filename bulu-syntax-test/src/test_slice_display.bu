func main() {
    println("=== Test affichage des slices ===")
    
    // Test slice vide
    let empty = make([]int32)
    println("Slice vide: " + empty.toString())
    
    // Test slice avec 1 élément
    let single = make([]int32, 1)
    println("Slice 1 élément: " + single.toString())
    
    // Test slice avec plusieurs éléments
    let multiple = make([]int32, 5)
    println("Slice 5 éléments: " + multiple.toString())
    
    // Test avec différents types
    let bools = make([]bool, 3)
    println("Slice bools: " + bools.toString())
    
    let strings = make([]string, 4)
    println("Slice strings: " + strings.toString())
}