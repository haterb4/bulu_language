// Test de l'opérateur puissance **

func main() {
    println("=== Test de l'Opérateur Puissance ** ===")
    
    // Tests de base
    let result1 = 2.0 ** 3.0
    println("2.0 ** 3.0 = " + result1.toString())
    
    let result2 = 5.0 ** 2.0
    println("5.0 ** 2.0 = " + result2.toString())
    
    let result3 = 10.0 ** 0.0
    println("10.0 ** 0.0 = " + result3.toString())
    
    let result4 = 4.0 ** 0.5
    println("4.0 ** 0.5 = " + result4.toString())
    
    // Test avec des variables
    let base = 3.0
    let exponent = 4.0
    let result5 = base ** exponent
    println("3.0 ** 4.0 = " + result5.toString())
    
    println("Test terminé")
}