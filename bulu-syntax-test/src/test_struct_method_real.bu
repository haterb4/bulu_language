// Test de méthode avec logique réelle

struct TestStruct {
    value: float64
    
    func getValue(): float64 {
        return this.value
    }
    
    func isPositive(): bool {
        return this.value >= 0.0
    }
}

func main() {
    println("=== Test Méthode avec Logique Réelle ===")
    
    let s = TestStruct{value: 42.0}
    println("Structure créée avec value = 42.0")
    
    let val = s.getValue()
    println("getValue() retourne: " + val.toString())
    
    let positive = s.isPositive()
    println("isPositive() retourne: " + positive.toString())
    
    println("Test terminé")
}