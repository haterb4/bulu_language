struct Complex {
    real: int64,
    imag: int64,
    
    func add(other: Complex): Complex {
        return Complex {
            real: this.real + other.real,
            imag: this.imag + other.imag
        };
    }
    
    func getRealPart(): int64 {
        return this.real;
    }
    
    func getImagPart(): int64 {
        return this.imag;
    }
}

func main() {
    println("=== Complex Number Test ===");
    
    // Create two complex numbers: 3+4i and 1+2i
    let c1 = Complex { real: 3, imag: 4 };
    let c2 = Complex { real: 1, imag: 2 };
    
    // Add them: (3+4i) + (1+2i) = 4+6i
    let sum = c1.add(c2);
    
    // Get the parts
    let realPart = sum.getRealPart();
    let imagPart = sum.getImagPart();
    
    // Verify: should be 4 and 6
    let realCorrect = realPart == 4;
    let imagCorrect = imagPart == 6;
    
    if realCorrect {
        println("Real part is correct: 4");
    }
    
    if imagCorrect {
        println("Imaginary part is correct: 6");
    }
    
    if realCorrect {
        if imagCorrect {
            println("SUCCESS: (3+4i) + (1+2i) = 4+6i");
        }
    }
}
