// Basic arithmetic operations

export func add(a: int64, b: int64): int64 {
    return a + b;
}

export func subtract(a: int64, b: int64): int64 {
    return a - b;
}

export func multiply(a: int64, b: int64): int64 {
    return a * b;
}

export func divide(a: int64, b: int64): int64 {
    if b == 0 {
        return 0;
    }
    return a / b;
}

export func square(x: int64): int64 {
    return x * x;
}
