import { uppercase, concat, repeat } from "./lib.bu";

func main() {
    println("=== String Utils Demo ===");
    
    let text = "hello";
    let upper = uppercase(text);
    println("uppercase: " + upper);
    
    let combined = concat("Hello", " World");
    println("concat: " + combined);
    
    let repeated = repeat("*", 5);
    println("repeat: " + repeated);
}
